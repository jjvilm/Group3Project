<table id="patients-table">
  <thead>
    <tr>
      <th colspan="16"><h1 style="font-weight: bold; text-align: center;">Patient List</h1></th>
    </tr>
    <tr>
      <th>Creator Id</th>
      <th>Creator Name</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Birthdate</th>
      <th>Zipcode</th>
      <th>State</th>
      <th>Phone Number</th>
      <th>Create Date</th>
      <th>Insurance Type</th>
      <th>Test Type</th>
      <th>Doctor Service</th>
      <th>Lab Name</th>
      <th>Sample Status</th>
      <th><a href="#">Update</a></th>
      <th><a href="#">Delete</a></th>
    </tr>
  </thead>
  <tbody>
    <tr>
    </tr>
  </tbody>
</table>

<style>
table {
  border-collapse: collapse;
  width: 100%;
}

th, td {
  text-align: left;
  padding: 8px;
  border: 1px solid #ddd;
}

th {
  background-color: #f2f2f2;
  border: 1px solid #ddd;
}

tr:nth-child(even) {
  background-color: #f2f2f2;
}

a {
  text-decoration: none;
  color: blue;
}
</style>

<script>
function addPatientRow(patient) {
  var table = document.getElementById("patients-table");
  var row = table.insertRow();
  var keys = Object.keys(patient);
  for (var i = 0; i < keys.length; i++) {
    var cell = row.insertCell();
    cell.innerHTML = patient[keys[i]];
  }
  var updateLink = "<a href='#'>Update</a>";
  var deleteLink = "<a href='#'>Delete</a>";
  row.insertCell().innerHTML = updateLink;
  row.insertCell().innerHTML = deleteLink;
}
</script>
